{
  "ignorePatterns": ["flow-typed", "node_modules/", "scripts"],
  "env": {
    "es6": true,
    "es2021": true,
    "browser": true
  },
  "extends": ["airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  }
}
